<%- include('header')%>

    <div class="container pt-3 my-5">
        <div class="row">
          <h2>
           Password Category Lists
          </h2>
    
          <table class="table table-striped table-light">
            <thead>
              <tr>
                <th scope="col">Password Category</th>
                <th scope="col">Action</th>
              </tr>
            </thead>
            <tbody>
              <%if(records.length > 0){
                records.forEach(function(row){
                %>

              <tr>
                <td>
                  <%=row.password_category%>
                </td>
                
                <td>
                  <a href="#"><i class="fa fa-eye"></i></a>
                  <a  href="/passwordCategory/edit/<%=row._id%>"><i class="fa fa-edit"></i></a>
                  <a  href="/passwordCategory/delete/<%=row._id%>" onclick="return confirm('Are you sure?')"><i class="fa fa-trash"></i></a>
                </td>
              </tr>
              
                <%})}%>
            </tbody>
          </table>
        </div>
      </div>
  </body>
</html>
